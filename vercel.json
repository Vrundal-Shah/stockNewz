{
  "version": 2,
  "builds": [
    {
      "src": "/flask_app/run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/flask_app/stocks/(.*)",
      "dest": "/flask_app/stocks"
    },
    {
      "src": "/flask_app/users/(.*)",
      "dest": "/flask_app/users"
    }
  ]
}
